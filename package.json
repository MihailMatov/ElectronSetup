{
  "name": "my-electron-app",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "compile": "npx electron-packager . my-electron-app --platform=win32 --arch=x64 --out=dist"
  },
  "devDependencies": {
    "electron": "^31.0.1"
  },
  "dependencies": {
    "electron-packager": "^17.1.2"
  },
  "build": {
    "appId": "com.example.myapp",
    "win": {
      "target": "nsis"
    }
  }
}
